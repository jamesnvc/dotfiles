# key: plunit
# name: plunit tests
# --
:- module(${1:`(file-name-base (buffer-file-name))`}, []).

:- use_module(library(plunit)).
:- use_module(${1:$(s-replace-regexp "_test$" "" yas-text)}).

:- begin_tests(${1:$(s-replace-regexp "_test$" "" yas-text)}).

$0

:- end_tests(${1:$(s-replace-regexp "_test$" "" yas-text)}).