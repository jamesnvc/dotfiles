Host *
  VisualHostKey yes
  TCPKeepAlive no
  ServerAliveInterval 60
  ServerAliveCountMax 10
  ControlPersist 4h
  ControlMaster auto
  ControlPath ~/.ssh/master-%r@%h:%p
  AddKeysToAgent yes

